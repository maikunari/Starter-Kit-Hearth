{
  "name": "starter-kit-v4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:eleventy & npm run watch:css",
    "build": "npm run build:eleventy && npm run build:css && npm run critical",
    "build:css": "postcss CSS/tailwind.css -o _site/css/style.css",
    "watch:css": "postcss CSS/tailwind.css -o _site/css/style.css --watch",
    "start:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "critical": "critical _site/index.html --base _site --inline > _site/index-critical.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OakHarbor/Starter-Kit-v4-Eleventy-.git"
  },
