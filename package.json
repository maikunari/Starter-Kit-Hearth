  },
  "homepage": "https://github.com/OakHarbor/Starter-Kit-v4-Eleventy-#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.2",
    "critical": "^5.1.2"
  },
